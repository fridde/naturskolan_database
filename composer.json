{
    "name": "fridde/naturskolans_databas",
    "description": "A database system for Sigtuna Naturskola",
    "license": "MIT",
    "authors": [
        {
            "name": "fridde",
            "email": "f@hehl.se"
        }
    ],
    "require": {
        "nesbot/carbon": "1.22",
        "fridde/html": "dev-master",
        "fridde/mailer": "dev-master",
        "fridde/utility": "dev-master",
        "fridde/sms": "dev-master",
        "fridde/dumper": "dev-master",
        "eluceo/ical": "0.11.0",
        "fridde/essentials": "dev-master",
        "fridde/slacker": "dev-master",
        "fridde/orm": "dev-master",
        "altorouter/altorouter": "1.2",
        "guzzlehttp/guzzle": "6.2",
        "hashids/hashids": "2.0",
        "dotzero/googl": "0.2.0",
        "symfony/yaml": "3.3.0"
    },
    "require-dev": {
        "fzaninotto/faker": "1.6",
        "filp/whoops": "^2.1",
        "tracy/tracy": "^2.4"
    },
	"autoload": {
        "psr-4": { "Fridde\\": "src/" }
	},
	"minimum-stability": "dev",
	"prefer-stable": true,
    "config": {
        "preferred-install": {            
            "*": "dist"
        },
        "process-timeout": 60
    }
}
