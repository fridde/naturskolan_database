<meta http-equiv="Content-Type" content="text/html; charset=utf-8" /></meta>
{% block head %}
{% for css in CssFiles %}
    <link rel="stylesheet" href="{{ css }}">
{% endfor %}

{% if LiteralCss %}
    <style>
    {% for style in LiteralCss %}
        {{ style }}
    {% endfor %}
    </style>
{% endif %}
{% for js in JsFiles %}
    <script type="text/javascript" src="{{ js }}" async></script>
{% endfor %}

{% if LiteralJs %}
    <script>
    {% for script in LiteralJs %}
        {{ style }}
    {% endfor %}
    </script>
{% endif %}
<title>{{Title}}</title>
{% endblock %}
